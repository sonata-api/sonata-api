{
  "name": "@sonata-api/api",
  "version": "1.0.48",
  "description": "",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "env TS_NODE_COMPILER_OPTIONS=\"$(cat ../compilerOptions.json)\" mocha -r ts-node/register tests/*.spec.ts",
    "build": "pnpm build:cjs && pnpm build:esm",
    "build:cjs": "tsc",
    "build:esm": "swc src/* -d dist --strip-leading-paths -C module.type=es6 --out-file-extension=mjs && pnpm build:esm-transform",
    "build:esm-transform": "pnpm -w esm-transform $PWD/dist",
    "prepublishOnly": "pnpm build",
    "postinstall": "node dist/__scripts__/postinstall.js || true"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./presets": {
      "import": "./dist/presets/index.mjs",
      "require": "./dist/presets/index.js",
      "types": "./dist/presets/index.d.ts"
    },
    "./presets/*": "./dist/presets/*"
  },
  "files": [
    "dist"
  ],
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/jsonwebtoken": "^8.5.6"
  },
  "peerDependencies": {
    "@sonata-api/access-control": "*",
    "@sonata-api/common": "*",
    "@sonata-api/entrypoint": "*",
    "@sonata-api/http": "*",
    "@sonata-api/security": "*",
    "@sonata-api/builtins": "*",
    "@sonata-api/types": "*",
    "@sonata-api/validation": "*",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^6.1.0"
  },
  "optionalDependencies": {
    "mongodb-memory-server": "^8.10.2"
  }
}

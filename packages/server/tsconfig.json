{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "dist",
    "paths": {
      "@sonata-api/access-control": [
        "../access-control/dist"
      ],
      "@sonata-api/common": [
        "../common/dist/cjs"
      ],
      "@sonata-api/common/*": [
        "../common/dist/*"
      ],
      "@sonata-api/api": [
        "../api/dist"
      ],
      "@sonata-api/entrypoint": [
        "../entrypoint/dist"
      ],
      "@sonata-api/builtins": [
        "../builtins/dist"
      ],
      "@sonata-api/types": [
        "../types/dist"
      ],
      "@sonata-api/http": [
        "../http/dist"
      ],
      "@sonata-api/node-http": [
        "../node-http/dist"
      ]
    }
  },
  "include": [
    "@types/*.d.ts",
    "src"
  ]
}

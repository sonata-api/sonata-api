{
  "name": "@sonata-api/validation",
  "version": "1.0.14",
  "description": "## Installation",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "env TS_NODE_COMPILER_OPTIONS=\"$(cat ../compilerOptions.json)\" mocha -r ts-node/register tests/*.spec.ts",
    "build": "pnpm build:cjs && pnpm build:esm",
    "build:cjs": "tsc",
    "build:esm": "swc src/* -d dist --strip-leading-paths -C module.type=es6 --out-file-extension=mjs",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "peerDependencies": {
    "@sonata-api/common": "*",
    "@sonata-api/types": "*",
    "mongodb": "*"
  }
}
